<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
            <record id="ir_cron_auto_cancel_expired_quote" model="ir.cron">
            <field name="name">Cancel Expired Quotes</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="nextcall" eval="(DateTime.now().replace(hour=0, minute=0, second=0) + timedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="doall" eval="False"/>
            <field name="model_id" ref="sale.model_sale_order"/>
            <field name="code">model._auto_cancel_expired()</field>
            <field name="state">code</field>
        </record>
    </data>
</odoo>
